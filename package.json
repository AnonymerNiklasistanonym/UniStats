{
  "name": "uni-stats",
  "version": "0.0.1",
  "description": "Visualize your current University progress (made for computer science students of the university of Stuttgart)",
  "author": "AnonymerNiklasistanonym",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnonymerNiklasistanonym/UniStats.git"
  },
  "url": "https://github.com/AnonymerNiklasistanonym/UniStats",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p . && npm run copy-static-assets",
    "build_on_update": "tsc -w -p .",
    "start_js": "node dist/src/index.js",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec ./node_modules/.bin/ts-node src/index.ts",
    "tslint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json --fix",
    "typedoc": "./node_modules/.bin/typedoc --tsconfig ./tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "createTsTypeFromJsonSchema": "node templates/createTsTypeFromJsonSchema.js"
  },
  "dependencies": {
    "nodemon": "^1.19.4",
    "ts-node": "^8.4.1",
    "json-schema-to-typescript": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.5",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.4",
    "typedoc": "^0.15.0",
    "shelljs": "^0.8.3"
  }
}
