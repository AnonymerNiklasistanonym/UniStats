{
  "extends": "tslint:all",
  "rules": {
    "only-arrow-functions": false,
    "no-inferrable-types": false,
    "newline-per-chained-call": false,
    "no-namespace": false,
    "max-line-length": [
      true,
      80
    ],
    "ban-ts-ignore": false,
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ],
    "no-console": [ true,"log"]
  }
}
